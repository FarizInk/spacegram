{
	"name": "sveltekit-hono",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "bunx --bun vite dev",
		"build": "bunx --bun vite build",
		"preview": "bunx --bun vite preview",
		"check": "bunx --bun svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "bunx --bun svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "bunx --bun vitest",
		"lint": "bunx --bun prettier --check . && eslint .",
		"format": "bunx --bun prettier --write .",
		"studio": "prisma studio",
		"migrate-dev": "prisma migrate dev",
		"migrate-deploy": "prisma migrate deploy",
		"migrate-reset": "prisma migrate reset",
		"migrate-status": "prisma migrate status",
		"generate": "prisma generate"
	},
	"prisma": {
		"schema": "prisma/schema.prisma",
		"seed": "tsx prisma/seed.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/bcrypt": "^5.0.2",
		"@types/bun": "^1.0.8",
		"@types/eslint": "8.56.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.17",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.35",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^1.2.0"
	},
	"dependencies": {
		"@hono/node-server": "^1.8.1",
		"@hono/sentry": "^1.0.1",
		"@prisma/client": "^5.10.2",
		"@prisma/studio": "^0.499.0",
		"@sveltejs/adapter-node": "^4.0.1",
		"@tailwindcss/typography": "^0.5.10",
		"axios": "^1.6.7",
		"bcrypt": "^5.1.1",
		"bits-ui": "^0.18.2",
		"hono": "^4.0.2",
		"phosphor-svelte": "^1.4.2",
		"prisma": "^5.10.2",
		"svelte-sonner": "^0.3.18",
		"telegram": "^2.19.20",
		"tsx": "^4.7.1",
		"ulid": "^2.3.0"
	}
}
